#include <stdio.h>

int main()
{
    int size,key,i,found;
    scanf("%d",&size);
    int arr[size];
    for(i=0;i<size;i++){
     scanf("%d",&arr[i]); 
    }
    scanf("%d",&key);
    for(i=0;i<size;i++)
    {
      if(arr[i]==key)
      {printf("%d",i);
      found=1;}
    }
    if(found!=1)
    printf("%d",-1);
     return 0;
}
