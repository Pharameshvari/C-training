#include <stdio.h>

int main()
{
    int size,i,count=0;
    scanf("%d",&size);
    int arr[size];
    for(i=0;i<size;i++)
    {
    scanf("%d",&arr[i]);
    }
    int key;
    scanf("%d",&key);

    for(i=0;i<size;i++)
    {
        if(arr[i]<key)
        count++;
    }
    printf("%d",count);

    return 0;
}
