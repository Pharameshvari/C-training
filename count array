#include <stdio.h>

int main()
{
    int size,i,count=0,count1;
    scanf("%d",&size);
    int arr[size];
    for(i=0;i<size;i++)
    scanf("%d",&arr[i]);
    for(i=0;i<size;i++){
        if(arr[i]==0)
        count++;
        else
        count1++;
    }
     printf("%d %d",count,count1);

    return 0;
}
